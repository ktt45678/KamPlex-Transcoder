    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.7.0",
    "prettier": "2.8.8",
    "supertest": "6.3.4",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
