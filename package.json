  "devDependencies": {
    "@nestjs/cli": "10.3.0",
    "@nestjs/schematics": "10.1.0",
    "@nestjs/testing": "10.3.0",
    "@swc/cli": "0.1.65",
    "@swc/core": "1.4.11",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/luxon": "3.4.1",
    "@types/node": "18.19.8",
    "@types/supertest": "2.0.16",
