    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/platform-fastify": "^9.1.2",
    "bull": "^4.10.0",
    "ffprobe-client": "^1.1.6",
    "luxon": "^3.0.4",
    "mkdirp": "^2.0.0",
    "mongoose": "^6.6.3",
    "nest-winston": "^1.8.0",
    "nodejs-snowflake": "^2.0.1",
    "reflect-metadata": "^0.1.13",
